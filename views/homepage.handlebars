<!-- Loop over each object in the projects array and show all pets -->
<div id="pet-gallery" class="d-flex justify-content-evenly">
  {{#each cars as |car| }}
  <div id="pet-cards" class="card row mb-4 project">
    <div class="col-md-7">
      <h2>
        <a href="/car/{{car.id}}"> {{car.title}}</a>
      </h2>
      
      <p><b>Sold by:</b> {{{car.user.name}}}</p>
    </div>
    <div class="col-md-7">
      <p>
        <b>Manufacturer:</b> {{car.manufacturer}}
      </p>
    </div> <div class="col-md-7">
      <p>
        <b>Description:</b> {{car.description}}
      </p>
    </div>
     </div> <div class="col-md-7">
      <p>
        <b>Mileage:</b> {{car.mileage}}
      </p>
    </div>
     <div class="col-md-7">
      <p>
        <b>Price:</b> {{car.price}}
      </p>
    </div>
    
   



  </div>

  {{/each}}
</div>

